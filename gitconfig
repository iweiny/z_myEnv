[alias]
        co = checkout
        st = status
        br = branch
        brv = branch -vv
        brr = branch -r
        di = diff
        dic = diff --color
        sh = show
        shc = show --color
	lg = log --abbrev-commit --pretty=oneline --decorate --color
	lgg = log --graph --abbrev-commit --pretty=oneline --decorate --color
	lgp = log -p --color
	authors = log --pretty=format:'%h  :  %aN  :  %s'
	emails = log --pretty=format:'%aN <%aE> %s'
	changelog = log --pretty=format:'* %s'
	fixes = log --pretty=fixes
	rbcont = rebase --continue
	rba = rebase --abort
	rbsk = rebase --skip
	pullr = pull --rebase
	chp = cherry-pick
	contains = branch --contains

